#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int sum=0;
	    int c[n+1];
	    float sol[n+1];
	    for(int i=1;i<n+1,i++){
	        cin>>c[i];
	        sum+=c[i];
	    }
	    for(int i=1;i<n+1,i++){
	        sol[i]=(((float)sum)/n-1) - c[i];
	        cout<<sol[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}

